{
  "name": "area-server",
  "version": "0.0.1",
  "description": "Area API server",
  "homepage": "https://github.com/bobis33/Area-2026/packages/server",
  "private": true,
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bobis33/Area-2026.git",
    "directory": "packages/server"
  },
  "scripts": {
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:format": "prisma format",
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cron-parser": "^5.4.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-github2": "^0.1.12",
    "passport-gitlab2": "^5.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.8.0",
    "passport-spotify": "^2.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.10",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.3",
    "@types/node": "^25.0.3",
    "@types/passport-discord": "^0.1.15",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-oauth2": "^1.8.0",
    "@types/passport-spotify": "^2.0.4",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "json",
      "ts",
      "js"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.*spec\\.ts$",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@common/(.*)$": "<rootDir>/src/common/$1",
      "^@decorators/(.*)$": "<rootDir>/src/decorators/$1",
      "^@modules/(.*)$": "<rootDir>/src/modules/$1",
      "^@dto/(.*)$": "<rootDir>/src/dto/$1",
      "^@interfaces/(.*)$": "<rootDir>/src/interfaces/$1",
      "^@pcg/(.*)$": "<rootDir>/prisma/generated/$1"
    },
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
